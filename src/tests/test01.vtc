varnishtest "Test ip2location vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import example from "${vmod_topbuild}/src/.libs/libvmod_ip2location.so";

	sub vcl_deliver {
		set resp.http.hello = example.hello("World");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.hello == "Hello, World"
}

client c1 -run
